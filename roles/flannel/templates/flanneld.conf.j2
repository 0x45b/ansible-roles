{% if etcd_tls %}
FLANNEL_ETCD_CAFILE="{{ flannel_tls_dir }}/{{ flannel_etcd_cafile }}"
FLANNEL_ETCD_CERTFILE="{{ flannel_tls_dir }}/{{ flannel_etcd_certfile }}"
FLANNEL_ETCD_KEYFILE="{{ flannel_tls_dir }}/{{ flannel_etcd_keyfile }}"
{% endif %}
FLANNEL_ETCD_ENDPOINTS="{{ flannel_etcd_endpoints }}"
FLANNEL_ETCD_PREFIX="{{ flannel_etcd_prefix | default("/coreos.com/network") }}"
FLANNEL_OPTIONS="{{ flannel_options | default("") }}"